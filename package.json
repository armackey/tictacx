{
  "name": "tictacx",
  "nativescript": {
    "id": "org.nativescript.tictacx",
    "tns-android": {
      "version": "5.3.1"
    },
    "tns-ios": {
      "version": "5.3.1"
    }
  },
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "mobile": "tns run --bundle",
    "preview": "tns preview --bundle",
    "dev-serve": "npm run dev-build && ng build && ng run tictacx:server && npm run serve",
    "prod": "npm run dev-build && ng build --prod && ng run tictacx:server && npm run serve",
    "heroku-postbuild": "ng build --prod",
    "serve": "node server/dist/index.js",
    "dev-build": "tsc ./server/**/*.ts"
  },
  "private": true,
  "dependencies": {
